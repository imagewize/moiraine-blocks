{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "imagewize/slide",
    "version": "1.0.0",
    "title": "Slide",
    "parent": [
        "imagewize/carousel"
    ],
    "category": "design",
    "icon": "slides",
    "description": "A single slide container that can hold any content within the carousel.",
    "supports": {
        "html": false
    },
    "textdomain": "imagewize",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "attributes": {
        "slideId": {
            "type": "string",
            "default": ""
        }
    }
}